<odoo>
    <record id="view_uom_bulk_calc_form" model="ir.ui.view">
        <field name="name">uom.bulk.calc.wizard.form</field>
        <field name="model">uom.bulk.calc.wizard</field>
        <field name="arch" type="xml">
            <form string="Bulk UoM Calculator">
                <group>
                    <field name="total_weight"/>
                    <field name="weight_unit"/>
                    <field name="weight_per_unit"/>
                    <field name="unit_count" readonly="1"/>
                </group>
            </form>
        </field>
    </record>
</odoo>